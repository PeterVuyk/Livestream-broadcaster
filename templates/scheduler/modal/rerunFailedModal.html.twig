<div class="modal" id="js-rerun-failed-schedule-modal-{{ scheduledItem.id }}" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{% trans %}action.rerun_failed_schedule.title{% endtrans %}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>{% trans %}confirm.rerun_failed_schedule{% endtrans %}</p>
            </div>
            <div class="modal-footer">
                <button onclick="location.href='{{ path('scheduler_unwreck', {'scheduleId': scheduledItem.id}) }}'"
                        type="button" class="btn btn-primary">{% trans %}action.rerun_failed_schedule{% endtrans %}</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">{% trans %}action.cancel{% endtrans %}</button>
            </div>
        </div>
    </div>
</div>
